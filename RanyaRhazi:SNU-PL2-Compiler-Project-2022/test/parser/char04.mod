module char04;

var a,b,c,d: char;

begin
  if (a = b) then a := '0' end;
  if (a # b) then a := '1' end;
  if (a < b) then a := '7' end;
  if (a > b) then a := '6' end;
  //if (a + b < '9') then a := '6' end;
  //if (a * b = '0') then a := '0' end
end char04.
